// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Utilisteur {
  id    Int @id @default(autoincrement())
  nom   String
  email String
  tel   String
}

model Appareil {
  id             Int @id @default(autoincrement())
  ref            String
  label          String
  estFonctionnel Boolean
}

model Tag {
  id    Int @id @default(autoincrement())
  label String
}